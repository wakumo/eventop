{
  "name": "avacuscc-events-mq",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "engines": {
    "npm": "please-use-yarn",
    "node": ">=16.16.0",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=1 --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "ts-node": "ts-node -r tsconfig-paths/register -r dotenv/config",
    "ts-node-esm": "ts-node-esm -r dotenv/config -r tsconfig-paths/register",
    "typeorm": "ts-node-esm -r tsconfig-paths/register ./node_modules/.bin/typeorm -d ./src/config/typeorm.config.ts",
    "execute-tsnode": "yarn run ts-node-esm src/execute_script",
    "execute": "node dist/src/execute_script.js",
    "db:schema:drop": "yarn typeorm schema:drop",
    "db:migrate:generate": "yarn typeorm migration:generate",
    "db:migrate:up": "yarn typeorm migration:run",
    "migrate_and_start": "yarn run ts-node-esm create_db.ts; yarn db:migrate:up; yarn start",
    "migrate_and_start_prod": "node dist/create_db.js; yarn db:migrate:up; yarn start:prod",
    "db:migrate:down": "yarn typeorm migration:revert"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.3.0",
    "@liaoliaots/nestjs-redis": "^9.0.3",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/terminus": "^9.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/lodash": "^4.14.201",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "bull": "4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "file-type": "^18.0.0",
    "ioredis": "^5.2.3",
    "lodash": "^4.17.21",
    "moralis": "^2.6.5",
    "nest-commander": "^3.1.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-jest-resolver": "^2.0.0",
    "typeorm": "^0.3.10",
    "web3": "v4.0.1-alpha.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0-0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/amqplib": "^0.10.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "28.1.8",
    "@types/jest-when": "^3.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-when": "^3.5.2",
    "multer": "^1.4.5-lts.1",
    "nock": "^13.2.9",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "resolver": "ts-jest-resolver",
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@/src/(.*)$": "<rootDir>/$1",
      "^src/(.*)$": "<rootDir>/$1",
      "^@/test/(.*)$": "<rootDir>/../test/$1"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "peerDependencies": {
    "bull": "^4.10.0"
  }
}
